@model List<DBA.Model.CKRecord>


    <table class="tablelist">
    	<thead>
    	<tr>
        <th>序号<i class="sort"><img src="/Content/images/px.gif" /></i></th>
        <th>化妆品名</th>
           <th>货号</th> 
            <th>零售价</th> 
             <th>数量</th> 
              <th>总价</th> 
            <th>总利润</th> 
             <th>操作员</th> 
            <th>时间</th> 

            
       
        </tr>
        </thead>
        <tbody>
              @if (Model != null && Model.Count > 0)
{
    int i = 0; 
    
            foreach (var item in Model)
        {
            i++;    
        <tr>
        <td>@i</td>
        <td>@item.BookName</td>
            <td>@item.ISBN</td>
              <td>@item.Str3</td>
             <td>@item.Num</td>
               <td>@item.Price</td>
             <td>@item.Str2</td>
             <td>@item.CreateUserName</td>
            
             <td>@item.Time</td>
    
        </tr>    
            }
} 
        </tbody>
    </table>
    


    

