@model List<DBA.Model.CKRecord>


    <table class="tablelist">
    	<thead>
    	    <tr>
                <th>序号<i class="sort"><img src="/Content/images/px.gif" /></i></th>
                <th>化妆品名</th>
                <th>数量</th> 
                <th>销售额</th> 
                <th>总利润</th> 
            </tr>
        </thead>
        <tbody>
              @if (Model != null && Model.Count > 0)
              {
                int i = 0; 
    
                        foreach (var item in Model)
                        {
                                i++;    
                                <tr>
                                    <td>@i</td>
                                    <td>@item.BookName</td>
                                    <td>@item.Num</td>
                                    
                                                                    @*<td>@item.Str3</td>零售价*@                                   
                                                                    @*<td>@item.Price</td>销售额*@
                                                                    @*<td>@item.Str2</td>利润*@
                                    <td>@item.CreateUserName</td>
                                    <td>@item.Time</td>
                                    <td><a href="/CKRecord/Create?ID=@item.ID"   >查看</a> </td>
          
           
                                    
                                </tr>    
                        }
               } 
        </tbody>
    </table>
    


    

