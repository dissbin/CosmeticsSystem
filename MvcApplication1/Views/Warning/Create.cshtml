@model DBA.Model.Warning
@{
    ViewBag.Title = "Create";
    Layout = null;
}


<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title></title>
    <script src="../../Scripts/jquery-1.7.1.min.js"></script>
<link href="/Content/css/style.css" rel="stylesheet" type="text/css" />
</head>

<body class="add_test_img"  >

	<div class="place">
    <span>位置：</span>
    <ul class="placeul">
    <li><a href="#">首页</a></li>
    <li><a href="#">添加预警阀值</a></li>
    </ul>
    </div>
    
    <div class="formbody">
    
    <div class="formtitle"><span>添加预警阀值</span></div>
    <form method="post" action="/Warning/Edit">
    <ul class="forminfo">
               <li><label>预警名称</label>@Html.TextBoxFor(m => m.WarnType, new { @class = "dfinput" })</li>
           <li><label>最小值</label>@Html.TextBoxFor(m => m.Min, new { @class = "dfinput" })</li>
       <li><label>最大值</label>@Html.TextBoxFor(m => m.Max, new { @class = "dfinput" })</li>

        @Html.HiddenFor(m=>m.ID)
    <li><label>&nbsp;</label><input name="" style="border:0;" type="submit" class="btn" value="确认保存"/></li>
    </ul>
    </form>
    
    </div>


</body>

</html>
