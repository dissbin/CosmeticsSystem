@model List<DBA.Model.Category>


    <table class="tablelist">
    	<thead>
    	<tr>
        <th>序号<i class="sort"><img src="/Content/images/px.gif" /></i></th>
        <th>供货商</th>
             <th>联系方式</th>
            <th>地址</th>
        <th>操作</th>
        </tr>
        </thead>
        <tbody>
              @if (Model != null && Model.Count > 0)
              {
                        int i = 0; 
       
                        foreach (var item in Model)
                        {
                            i++;    
             
                        <tr>
                            <td>@i</td>
                            <td>@item.CategoryName</td>
                                 <td>@item.LXFS</td>
                               <td>@item.DZ</td>
                            <td>
                                <a href="/Category/GCreate?ID=@item.ID"   >修改</a> 
                                <a href="#" onclick="Del(@item.ID)"   >删除</a> 
                            </td>
                        </tr>    
              }
              
}       </tbody>
    </table>
    


    

