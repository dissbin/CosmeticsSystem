<script type="text/ecmascript">
    function Del(id) {
          if (confirm("确定删除该项目？")) {
                $.ajax({
                    type: "post",
                    traditional: true,
                    url: "/Project/Del",
                    data: { ID: id },
                    datatpye: "json",
                    success: function (data) {
                        if (data) {
                           alert("删除成功！")
                                window.location.reload();
                        }
                    }
                });
            }
    }
</script>